fun max(a: num, b: num): num {
  if (a > b) return a
  return b
}

fun min(a: num, b: num): num {
  if (a < b) return a
  return b
}

fun abs(x: num): num {
  if (x < 0) return -x
  return x
}

external class Math {
  static fun random(): num

  static fun randomInt(max: num): num

  static fun sqrt(x: num): num

  static fun log(x: num): num

  static fun sin(x: num): num

  static fun cos(x: num): num
}
